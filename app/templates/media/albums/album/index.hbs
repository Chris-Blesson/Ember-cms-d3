{{#link-to 'media.albums' class='link dark-gray dim'}}
  &lt; All albums
{{/link-to}}

<h1>{{model.title}}</h1>

<div class="flex flex-wrap">
  {{#each model.images as |image|}}
    {{#link-to 'media.albums.album.image' image class='dim'}}
      <img src={{image.url}} class='w4 h4 mr4 mb4'>
    {{/link-to}}
  {{/each}}
</div>
